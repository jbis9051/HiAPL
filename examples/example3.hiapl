<!DOCTYPE hiapl>
<express init="true">
    <require>
        <arg>express</arg>
    </require>
</express>

<app init="true">
    <express></express>
</app>

<port init="true">
    <arg>3000</arg>
</port>

<app>
    <get>
        <arg>/</arg>
        <eval>
            <arg>(req,res) => res.send('Hello World');</arg>
        </eval>
        <!--func>
            <params><param>req</param><param>res</param></params>
            <res>
                <send>
                    <arg>Hello World</arg>
                </send>
            </res>
        </func-->
    </get>
</app>

<app>
    <listen>
        <port/>
    </listen>
</app>
